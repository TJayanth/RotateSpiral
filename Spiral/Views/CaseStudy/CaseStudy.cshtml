@model Spiral.Models.CaseStudyModel

@{
    ViewBag.Title = "Case Study";
}
<div class="text-center">
    <ul>
        <li> @Html.ActionLink("Back to DashBoard", "Index") |  @Html.ActionLink("Generic Type", "GenericType")</li>
    </ul>
</div>
<div class="form-group">
    <h3 class="text-center">Input</h3>
    <table class="table table-bordered">
        @for (int i = 0; i < 5; i++)
        {
            <tr>
                @for (int j = 0; j < 5; j++)
                {
                    <td>
                        @Html.DisplayFor(m => m.inputArray[i][j])
                    </td>
                }
            </tr>
        }
    </table>
</div>
<hr />
<div class="form-group">
    <div class="col-md-6">
        <h3>Output: 1</h3>
        <table class="table table-bordered">
            @for (int i = 0; i < 5; i++)
            {
                <tr>
                    @for (int j = 0; j < 5; j++)
                    {
                        <td>
                            @Html.DisplayFor(m => m.outPut1[i][j], new { @class = "col-md-2" })
                        </td>
                    }
                </tr>
            }
        </table>
    </div>

    <div class="col-md-6">
        <h3>Output: 2</h3>
        <table class="table table-bordered">
            @for (int i = 0; i < 5; i++)
            {
                <tr>
                    @for (int j = 0; j < 5; j++)
                    {
                        <td>
                            @Html.DisplayFor(m => m.outPut2[i][j], new { @class = "col-md-2" })
                        </td>
                    }
                </tr>
            }
        </table>
    </div>
</div>
